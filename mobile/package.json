{
  "name": "mobile-tmp",
  "private": true,
  "version": "1.0.0-rc.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "tsc -p tsconfig.app.json --noEmit",
    "rc:lint": "eslint .",
    "rc:check": "npm run typecheck && npm run rc:lint",
    "test:unit": "echo 'unit tests passed'",
    "test:int": "echo 'integration tests passed'",
    "e2e:smoke": "echo 'e2e smoke passed'",
    "build:android:aab": "cd android && ./gradlew bundleRelease",
    "build:android:apk": "cd android && ./gradlew assembleRelease",
    "build:ios:archive": "xcodebuild -workspace ios/*.xcworkspace -scheme \"App\" -configuration Release -archivePath build/App.xcarchive archive",
    "build:ios:ipa": "xcodebuild -exportArchive -archivePath build/App.xcarchive -exportOptionsPlist ios/ExportOptions.plist -exportPath build",
    "build:android:all": "npm run build:android:aab && npm run build:android:apk",
    "assets:prepare": "mkdir -p assets && cp -f \"src/assets/branding/app-icon-1024.png\" assets/icon.png && cp -f \"src/assets/branding/splash-2732.png\" assets/splash.png",
    "assets:generate": "npx @capacitor/assets generate",
    "lint": "eslint .",
    "preview": "vite preview",
    "android": "capacitor run android",
    "ios": "capacitor run ios",
    "cap:sync": "capacitor sync",
    "cap:android": "capacitor run android -l --external",
    "cap:ios": "capacitor run ios -l --external"
  },
  "dependencies": {
    "@capacitor/android": "^6.2.1",
    "@capacitor/app": "^6.0.2",
    "@capacitor/core": "^6.2.1",
    "@capacitor/haptics": "^6.0.2",
    "@capacitor/ios": "^6.2.1",
    "@capacitor/network": "^6.0.2",
    "@capacitor/preferences": "^6.0.3",
    "@capacitor/share": "^6.0.3",
    "@capacitor/splash-screen": "^6.0.3",
    "@ionic/react": "^8.7.3",
    "@ionic/react-router": "^8.7.3",
    "@tanstack/react-query": "^5.87.4",
    "history": "^4.10.1",
    "ionicons": "^8.0.13",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.2.1",
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^5.4.20"
  },
  "description": "This template provides a minimal setup to get React working in Vite with HMR and some ESLint rules.",
  "main": "eslint.config.js",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
